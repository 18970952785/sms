<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.malicn.server.dao.order.UnLoadListDao" >
  <resultMap id="UnLoadListMap" type="com.malicn.server.domain.order.UnLoadList" >
    <id column="order_id" property="orderId" jdbcType="BIGINT" />
    <result column="unload_pics" property="unloadPics" jdbcType="VARCHAR" />
    <result column="cargo_pics" property="cargoPics" jdbcType="VARCHAR" />
    <result column="driver_sign" property="driverSign" jdbcType="VARCHAR" />
    <result column="driver_id" property="driverId" jdbcType="BIGINT" />
    <result column="driver_name" property="driverName" jdbcType="VARCHAR" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
    <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
  </resultMap>

  <insert id="saveUnLoadList" parameterType="com.malicn.server.domain.order.UnLoadList">
      INSERT INTO ml_unload_list(order_id, unload_pics, cargo_pics, driver_sign, driver_id, driver_name, create_time, update_time)
      VALUES (#{orderId}, #{unloadPics}, #{cargoPics}, #{driverSign}, #{driverId}, #{driverName}, now(), now())
  </insert>

  <insert id="insertSelective" parameterType="com.malicn.server.domain.order.UnLoadList" >
    insert into ml_unload_list
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="orderId != null" >
        order_id,
      </if>
      <if test="unloadPics != null" >
        unload_pics,
      </if>
      <if test="cargoPics != null" >
        cargo_pics,
      </if>
      <if test="driverSign != null" >
        driver_sign,
      </if>
      <if test="driverId != null" >
        driver_id,
      </if>
      <if test="driverName != null" >
        driver_name,
      </if>
      <if test="createTime != null" >
        create_time,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="orderId != null" >
        #{orderId,jdbcType=BIGINT},
      </if>
      <if test="unloadPics != null" >
        #{unloadPics,jdbcType=VARCHAR},
      </if>
      <if test="cargoPics != null" >
        #{cargoPics,jdbcType=VARCHAR},
      </if>
      <if test="driverSign != null" >
        #{driverSign,jdbcType=VARCHAR},
      </if>
      <if test="driverId != null" >
        #{driverId,jdbcType=BIGINT},
      </if>
      <if test="driverName != null" >
        #{driverName,jdbcType=VARCHAR},
      </if>
      <if test="createTime != null" >
        #{createTime,jdbcType=TIMESTAMP},
      </if>
    </trim>
  </insert>

  <!-- 查询卸货清单 -->
  <select id="getByOrderId" parameterType="java.lang.Long" resultMap="UnLoadListMap">
    SELECT unload_pics,cargo_pics,driver_sign,driver_name,create_time FROM ml_unload_list
    where order_id=#{orderId}
  </select>
</mapper>