<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.malicn.server.dao.cmsorder.CmsOrderDetailsDao" >
  <resultMap id="OrderDetailsMap" type="com.malicn.server.domain.cmsorder.CmsOrderDetails" >
    <id column="order_id" property="orderId" jdbcType="BIGINT" />
    <result column="cargo_ctgry_name" property="cargoCtgryName" jdbcType="VARCHAR" />
    <result column="product_name" property="productName" jdbcType="VARCHAR" />
    <result column="pieces_num" property="piecesNum" jdbcType="INTEGER" />
    <result column="single_weight" property="singleWeight" jdbcType="DECIMAL" />
    <result column="factory_number" property="factoryNumber" jdbcType="VARCHAR" />
    <result column="production_time" property="productionTime" jdbcType="TIMESTAMP" />
    <result column="place_origin" property="placeOrigin" jdbcType="VARCHAR" />
    <result column="mark_pics" property="markPics" jdbcType="VARCHAR" />
  </resultMap>

  <sql id="AllColumn">
    order_id,cargo_ctgry_name,product_name,pieces_num,single_weight,factory_number,production_time,place_origin,mark_pics
  </sql>
    <!-- 查询订单详情 -->
    <select id="getOrderDetailsListByOrderId" parameterType="java.lang.Long" resultMap="OrderDetailsMap">
      select  <include refid="AllColumn" />
      from cms_order_details
      where  order_id= #{orderId}

    </select>
</mapper>